{
  "name": "Spotify Playback Speed",
  "description": "Adds a button and menu to manipulate playback speed on Spotify Web.",
  "version": "1.03",
  "manifest_version": 3,
  "author": "@rnikko",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://open.spotify.com/*"],
      "css": ["style.css"],
      "js": ["content-script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["https://open.spotify.com/*"],
      "resources": [
        "script.js",
        "style.css"
      ]
    }
  ]
}
